#include<iostream>
#include<cstring>

using namespace std;
int main()
{
	char arr[10086];
	while (gets(arr))
	{
		int n = strlen(arr);
		for (int i = 0; i < n; i++)
		{
			if (arr[i] == '/')
			{
				break;
			}
			else
			{
				for (int i = 0; i < n; i++)
				{
					if (arr[i] == '/')
					{
						break;
					}
					else
					{
						cout << arr[i];
					}

				}
			}
			cout << endl;
			break;
		}
	}
}
